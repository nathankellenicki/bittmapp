{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-consecutive-blank-lines": false,
        "space-before-function-paren": false,
        "no-bitwise": false,
        "trailing-comma": false,
        "max-line-length": false,
        "prefer-for-of": false,
        "typedef": true,
        "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"]
    },
    "rulesDirectory": []
}